#include <stdio.h>

int main(void) {
char at;
scanf("%c",&at);
	if((at=='a')||(at=='e')||(at=='i')||(at=='o')||(at=='u')||(at=='A')||(at=='E')||(at=='I')||(at=='O')||(at=='U'))
	{
		printf("the character %c is vowel",at);
	}
	else
	{
		printf("the character %c is consonant",at);	
	}
	// your code goes here
	return 0;
}
